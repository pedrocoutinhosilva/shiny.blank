{
  "theme": "pure",
  "button": "<button id={inputId} type='button' class='pure-button action-button' {disabled}>{content}</button>",
  "checkbox": "<label><input id={inputId} type='checkbox' class='nes-checkbox' {value} {disabled}/><span>{label}</span></label>",
  "dropdown": "<div class='pure-form'><select id={inputId} {disabled}/>{options}</select></div>",
  "option": "<option value='{value}'>{label}</option>"
}
