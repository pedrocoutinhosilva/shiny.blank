{
  "theme": "nes",
  "button": "<button id={inputId} type='button' class='action-button' {attributes}>{content}</button>",
  "checkbox": "<label><input id={inputId} type='checkbox' class='nes-checkbox' {value} {attributes}/><span>{label}</span></label>",
  "dropdown": "<div class='nes-select'><select id={inputId} {attributes}/>{options}</select></div>",
  "option": "<option value='{value}'>{label}</option>"
}
