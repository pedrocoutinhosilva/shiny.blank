{
  "theme": "default",
  "button": "<button id={inputId} type='button' class='action-button' {disabled}>{content}</button>",
  "checkbox": "<label><input id={inputId} type='checkbox' {value} {disabled}/><span>{label}</span></label>",
  "dropdown": "<select id={inputId} {disabled}/>{options}</select>",
  "option": "<option value='{value}'>{label}</option>"  
}
